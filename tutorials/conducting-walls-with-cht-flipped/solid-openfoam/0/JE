FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      JE;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 -2 0 0 0 1 0];

internalField   uniform (0 0 0);

boundaryField
{
    "(inlet|outlet|bot_surf|top_surf|ext_walls|top_int|bot_int|left_int|right_int)"
    {
        type            calculated;
        value           $internalField;
    }
}

