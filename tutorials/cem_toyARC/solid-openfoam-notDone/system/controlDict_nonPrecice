FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     cemSolidHTFoam;

startFrom       startTime;

startTime       0;

stopAt          endTime;

endTime         1;

deltaT          0.001;

writeControl    runTime;

writeInterval   0.001;

purgeWrite      10;

writeFormat     ascii;

writePrecision 	12;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable true;

