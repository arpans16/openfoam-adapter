FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      rho;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 -3 1 0 0 1 0];

internalField   uniform 1;

boundaryField
{
    "(inlet|outlet|ext_walls|top_int|bot_int|left_int|right_int)"
    {
        type            zeroGradient;
        value           $internalField;
    }
}


