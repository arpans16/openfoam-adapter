FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}

dimensions      [ 0 1 -1 0 0 0 0 ];

internalField   uniform ( 0.8 0 0 );

boundaryField
{
    inout
    {
        type            empty;
    }
    top
    {
        type            slip;
	value		$internalField;
    }
    bottom
    {
        type            slip;
	value		$internalField;
    }
    frontback
    {
        type            empty;
    }
}
